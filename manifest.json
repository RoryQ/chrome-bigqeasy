{
  "manifest_version": 3,
  "name": "BigQuery Easy",
  "version": "4.2.0",
  "description": "BigQuery but better ðŸš€",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/jquery_3_3_1.js", "scripts/utils.js", "scripts/popup.js", "scripts/inject.js"],
      "css": ["css/main.css"],
      "matches": ["https://console.cloud.google.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["scripts/jquery_3_3_1.js", "scripts/utils.js", "scripts/inject.js"],
      "matches": ["https://console.cloud.google.com/*"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "bigquery-easy@example.com"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "*://console.cloud.google.com/*",
    "*://clients6.google.com/*"
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  }
}
